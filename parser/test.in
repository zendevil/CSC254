   int n
   t := 9
   read n 
   int cp
   cp := 2
   while n > 0
       int found
       found := 0
       int cf1
       cf1 := 2
       int cf1s
       cf1s := cf1 * cf1
       while cf1s <= cp
           int cf2
	   cf2 := 2
           int pr
	   pr := cf1 * cf2
           while pr <= cp
               if pr == cp
                   found := 1
               end
               cf2 := cf2 + 1
               pr := cf1 * cf2
           end
           cf1 := cf1 + 1
           cf1s := cf1 * cf1
       end
       if found == 0
           write cp
           n := n - 1
       end
       cp := cp + 1
   end
